<?xml version="1.0" encoding="utf-8"?>
<layout   xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:focusable="true"
        android:clickable="true"
        android:focusableInTouchMode="true"
        android:background="@android:color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#F3F3F3"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvNotText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="12dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginRight="12dp"
                    android:layout_marginEnd="12dp"
                    android:paddingTop="8dp"
                    android:text="Lorem Ipsum is simply dummy text of the printing and typesetting industry.lorem Ipsum has been the industry's"
                    android:textColor="#333333"
                    android:textSize="12sp"
                    android:typeface="normal" />

                <TextView
                    android:textAllCaps="true"
                    android:id="@+id/tvProductNot"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tvNotText"
                    android:layout_marginTop="14dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginRight="12dp"
                    android:layout_marginEnd="12dp"
                    android:paddingBottom="8dp"
                    android:typeface="normal"
                    android:text="@string/txt_not_product_not"
                    android:textColor="@android:color/black"
                    android:textSize="12sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/tvAppoint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                android:textSize="14sp"
                android:text="@string/txt_not_appointments"
                android:textAlignment="center"
                android:textColor="@android:color/black"
                android:layout_below="@+id/ll"
                android:textStyle="normal"
                android:typeface="normal" />
            <com.kyleduo.switchbutton.SwitchButton
                android:layout_marginTop="8dp"
                android:id="@+id/sb_default_appoint"
                style="@style/SwitchButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:kswBackColor="@color/bright_turquoise"
                app:kswThumbColor="@android:color/white"
                android:layout_marginRight="12dp"
                android:layout_marginEnd="12dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/ll" />
            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#E1E1E1"
                android:layout_marginTop="8dp"
                android:layout_below="@+id/sb_default_appoint"/>
            <TextView
                android:id="@+id/tvFeedback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp"
                android:textSize="14sp"
                android:layout_below="@+id/view1"
                android:layout_marginTop="18dp"
                android:text="@string/txt_not_feedback"
                android:textAlignment="center"
                android:textColor="@android:color/black"
                android:textStyle="normal"
                android:typeface="normal" />

            <com.kyleduo.switchbutton.SwitchButton
                android:layout_below="@+id/view1"
                android:layout_marginTop="10dp"
                android:id="@+id/sb_default_feedback"
                style="@style/SwitchButtonStyle"
                app:kswBackColor="@color/bright_turquoise"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_marginEnd="12dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="8dp"
                app:kswThumbColor="@android:color/white" />
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#E1E1E1"
                android:layout_below="@+id/sb_default_feedback"/>
            <TextView
                android:id="@+id/tvBlogLikes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp"
                android:textSize="14sp"
                android:layout_below="@+id/sb_default_feedback"
                android:text="@string/txt_not_blog_likes"
                android:textAlignment="center"
                android:layout_marginTop="18dp"
                android:textColor="@android:color/black"
                android:textStyle="normal"
                android:typeface="normal" />
            <com.kyleduo.switchbutton.SwitchButton
                android:layout_marginTop="10dp"
                android:id="@+id/sb_default_blog_likes"
                style="@style/SwitchButtonStyle"
                android:layout_width="wrap_content"
                app:kswBackColor="@color/bright_turquoise"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_marginEnd="12dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="8dp"
                app:kswThumbColor="@android:color/white"
                android:layout_below="@+id/sb_default_feedback" />
            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="6dp"
                android:background="#E1E1E1"
                android:layout_marginTop="8dp"
                android:layout_below="@+id/sb_default_blog_likes"/>
            <TextView
                android:id="@+id/tvNotResetSet"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:padding="12dp"
                android:layout_below="@+id/view3"
                android:text="@string/txt_not_reset_not_settings"
                android:textColor="@android:color/holo_red_light"
                android:textSize="14sp"
                android:textStyle="bold"
                android:typeface="normal" />
        <View
            android:id="@+id/view4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#F3F3F3"
            android:layout_below="@+id/tvNotResetSet"/>
    </RelativeLayout>
</layout>